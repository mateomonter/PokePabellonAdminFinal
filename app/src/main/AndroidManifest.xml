        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PokePabellon"
        tools:targetApi="33">
        <activity
            android:name="com.mateomontero.pokepabellonAdmin.UploadImagesActivity"
            android:exported="false" />

        <activity
            android:name="com.mateomontero.pokepabellonAdmin.gestionnivel2.RemoveProductoActivity"
