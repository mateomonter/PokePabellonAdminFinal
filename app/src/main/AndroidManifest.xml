        <activity
            android:name="com.mateomontero.pokepabellonAdmin.gestionnivel2.ADminUsuariosActivity"
            android:exported="false" />
        <activity
            android:name="com.mateomontero.pokepabellonAdmin.gestionnivel2.PedidoActivity"
            android:exported="false" />
        <activity
            android:name="com.mateomontero.pokepabellonAdmin.gestionnivel1.GestionPedidosActivity"
            android:exported="false" />
        <activity
            android:name="com.mateomontero.pokepabellonAdmin.MainAdminActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>




        <activity
            android:name="com.mateomontero.pokepabellonAdmin.gestionnivel1.GestionProductoActivity"
            android:exported="false" />
        <activity
            android:name="com.mateomontero.pokepabellonAdmin.gestionnivel1.GestionUsarioActivity"
            android:exported="false" />







    </application>

</manifest>